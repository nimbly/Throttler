{
    "name": "nimbly\/throttler",
    "description": "A framework agnostic rate limiter.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Brent Scheffler",
            "email": "brent@brentscheffler.com"
        }
    ],
    "require": {
        "php": ">=7.2"
    },
    "autoload": {
        "psr-4": {
            "Throttler\\": "src"
        }
    },
    "require-dev": {
        "phpunit\/phpunit": "^7.3",
        "vimeo/psalm": "^3.0",
        "php-coveralls/php-coveralls": "^2.1",
        "predis/predis": "^1.1"
    },
    "autoload-dev": {
        "psr-4": {
            "Throttler\\Tests\\": "tests"
        }
    },
    "suggest": {
        "predis\/predis": "Use Redis as the Throttler storage engine.",
        "ext-PDO": "Use a database as the Throttler storage engine.",
        "ext-apcu": "Use APCU as the Throttler storage engine."
    },
    "scripts": {
        "test": "vendor\/bin\/phpunit"
    },
    "version": "1.0.2"
}
